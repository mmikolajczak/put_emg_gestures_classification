{
  "architecture":  "resnet",
  "force_cpu": false,
  "epochs": 100,
  "batch_size": 256,
  "shuffle": true,
  "nb_res_blocks": 6,
  "res_block_per_expansion": 2,
  "base_feature_maps": 32,
  "use_mixup": true,
  "alpha": 1,
  "val_split_size": 0.15,
  "base_lr": 1e-4,
  "max_lr": 1e-2,
  "use_early_stopping": true,
  "early_stopping_patience": 15,
  "optimizer": "radam",
  "use_lookahead": true
}